{
  "name": "preact-redux-isomorphic",
  "version": "0.6.0",
  "description": "preact-redux-isomorphic",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:client & npm run build:server",
    "build:client": "cross-env NODE_ENV=production BABEL_ENV=production TARGET=web BASEURL=/ webpack",
    "build:server": "cross-env NODE_ENV=production BABEL_ENV=production TARGET=node BASEURL=/ webpack",
    "build:serverless": "npm run build:serverless:server & npm run build:serverless:client",
    "build:serverless:client": "cross-env NODE_ENV=production BABEL_ENV=production TARGET=web RESOURCEPATH=_/ BASEURL=/ webpack",
    "build:serverless:server": "cross-env NODE_ENV=production BABEL_ENV=production TARGET=serverless RESOURCEPATH=_/ BASEURL=/ webpack",
    "dev": "cross-env NODE_ENV=development TARGET=web BASEURL=/ PORT=8080 HOST=localhost webpack-dev-server --inline --hot --progress",
    "dev:secure": "cross-env NODE_ENV=development TARGET=web BASEURL=/ PORT=8080 HOST=my-domain.com webpack-dev-server --inline --hot --progress --https",
    "po:export": "node ./scripts/intl export",
    "po:import": "node ./scripts/intl import",
    "graphql:merge": "node ./scripts/graphql --mode=merge --endpoint=http://159.203.96.223/graphql",
    "graphql:override": "node ./scripts/graphql --mode=override --endpoint=http://159.203.96.223/graphql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Bernd Wessels",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.0.0",
    "aws-serverless-express": "^3.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-react-intl": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.0.0",
    "babel-plugin-transform-react-jsx": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "bundle-loader": "^0.5.0",
    "clean-webpack-plugin": "^0.1.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "ejs": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.0",
    "glob": "^7.0.0",
    "html-webpack-plugin": "^2.0.0",
    "node-sass": "^4.0.0",
    "postcss-loader": "^2.0.0",
    "request": "^2.0.0",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.17.0",
    "sw-precache-webpack-plugin": "^0.11.0",
    "webpack": "^2.0.0",
    "webpack-dev-server": "^2.0.0",
    "yargs": "^8.0.0"
  },
  "dependencies": {
    "classnames": "^2.0.0",
    "history": "^4.0.0",
    "material-components-web": "^0.10.0",
    "material-design-icons": "^3.0.0",
    "preact": "^8.0.0",
    "preact-compat": "^3.0.0",
    "preact-mdc": "^0.1.0",
    "preact-redux": "^2.0.0",
    "ramda": "^0.23.0",
    "react-intl": "^2.0.0",
    "react-router": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.0.0",
    "redux-observable": "^0.14.0",
    "roboto-fontface": "^0.7.0",
    "rxjs": "^5.0.0",
    "undom": "^0.3.0",
    "xhr2": "^0.1.0"
  }
}
