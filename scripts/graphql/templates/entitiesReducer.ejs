/**
 * Import dependencies.
 */
import {combineReducers} from 'redux';

/**
 * Import local dependencies.
 */
<% for(var i=0; i<types.length; i++) {%>import {<%= types[i].name %>Reducer} from './<%= types[i].name %>Reducer';
<% } %>

/**
 * Export the entities store.
 */
export const entitiesReducer = combineReducers({
<% for(var i=0; i<types.length; i++) {%>  <%= types[i].Name %>: <%= types[i].name %>Reducer,
<% } %>
});
